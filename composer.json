{
    "name": "stepanenko3/laravel-helpers",
    "description": "Helpers for laravel",
    "keywords": [
        "laravel",
        "helpers"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-openssl": "*",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "laravel/pint": "^1.14",
        "ramsey/uuid": "^4.7",
        "vinkla/hashids": "^11.0 || ^12.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "tightenco/duster": "^2.7"
    }
}
